    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "core-js": "^3.15.1",
    "fs": "^0.0.1-security",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.27.2",
    "nuxt-property-decorator": "^2.9.1",
    "push-dir": "^0.4.1",
